        android:supportsRtl="true"
        android:theme="@style/Theme.GoBusker">
        <activity android:name=".fanForgot"></activity>
        <activity android:name=".buskerForgot" />
        <activity android:name=".fanHomePage" />
        <activity android:name=".buskerHomePage" />
        <activity android:name=".fanLogin" />
        <activity android:name=".fanRegister" />
        <activity android:name=".buskerRegister" />
        <activity android:name=".buskerLogin" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>